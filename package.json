{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "rm -rf dist && parcel index.html --open",
    "build": "rm -rf dist && parcel build index.html",
    "eslint": "./node_modules/.bin/eslint *.js",
    "test": "jest"
  },
  "dependencies": {
    "firebase": "^7.14.2",
    "minivents": "^2.2.0",
    "redux": "^4.0.5"
  },
  "staticFiles": {
    "staticPath": "src/assets",
    "watcherGlob": "**"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-static-files-copy": "^2.3.1"
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "max-len": [
        "error",
        100
      ]
    }
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
